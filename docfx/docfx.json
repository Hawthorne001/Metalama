{
  "metadata": [
    {
      "src": [
        {
          "files": [
              "Caravela.Framework/Caravela.Framework.csproj",
              "Caravela.TestFramework/Caravela.TestFramework.csproj",
              "Caravela.Framework.Sdk/Caravela.Framework.Sdk.csproj",
              ],
          "src": "c:/src/Caravela"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netstandard2.0"}
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "toc.yml"
        ],
        "src": "../conceptual"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
          "namespaces/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":[ 
      "default", 
      "packages/memberpage.2.56.6/content",
      "C:/src/Caravela.Home/code/Caravela.Documentation.DfmExtensions/bin/Debug"],
    "postProcessors": [],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}